{
  "name": "promethium-js",
  "version": "0.5.1",
  "description": "JavaScript framework built on top of lit-html",
  "source": "src/promethium-js.ts",
  "main": "promethium-js.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"parcel watch\" \"tsc --watch\"",
    "build": "concurrently \"parcel build\" \"tsc\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/promethiumjs/promethium.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "default": "./promethium-js.js"
    }
  },
  "bugs": {
    "url": "https://github.com/promethiumjs/promethium/issues"
  },
  "homepage": "https://github.com/promethiumjs/promethium#readme",
  "keywords": [
    "frontend",
    "ui",
    "lit-html",
    "javascript"
  ],
  "devDependencies": {
    "@parcel/core": "^2.7.0",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "concurrently": "^7.5.0",
    "parcel": "^2.7.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "lit-html": "^2.7.4"
  }
}
