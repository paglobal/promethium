{
  "name": "promethium",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev:package": "cd package && pnpm run dev",
    "dev:test": "cd test && pnpm run dev",
    "dev": "concurrently \"pnpm run dev:package\" \"pnpm run dev:test\" \"pnpm run test:package\"",
    "install:package": "cd package && pnpm install",
    "install:test": "cd test && pnpm install",
    "install": "concurrently \"pnpm run install:package\" \"pnpm run install:test\"",
    "build:package": "cd package && pnpm run build",
    "build:test": "cd test && pnpm run build",
    "build": "concurrently \"pnpm run build:package\" \"pnpm run build:test\"",
    "preview:test": "cd test && pnpm run preview",
    "on:package": "cd package &&",
    "on:test": "cd test &&",
    "publish:package": "cd package && pnpm publish",
    "link:package": "cd test && pnpm link ../package",
    "clean:package": "cd package && pnpm run clean",
    "test:package": "cd package && pnpm test",
    "coverage:package": "cd package && pnpm run coverage",
    "uninstall:package": "cd package && pnpm uninstall",
    "uninstall:test": "cd test && pnpm uninstall"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}
